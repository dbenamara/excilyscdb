<?xml version="1.0" encoding="UTF-8"?>
<web-app 
  xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
  version="3.0">

	<servlet>
		<servlet-name>ServletEditComputer</servlet-name>
		<servlet-class>servlet.ServletEditComputer</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ServletEditComputer</servlet-name>
		<url-pattern>/EditComputer</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>ServletListComputers</servlet-name>
		<servlet-class>servlet.ServletListComputers</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ServletListComputers</servlet-name>
		<url-pattern>/ListComputers</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>ServletAddComputer</servlet-name>
		<servlet-class>servlet.ServletAddComputer</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ServletAddComputer</servlet-name>
		<url-pattern>/AddComputer</url-pattern>
	</servlet-mapping>
	


</web-app>

